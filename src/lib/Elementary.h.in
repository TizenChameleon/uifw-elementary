    *
    * @code
    * li = elm_list_add(win);
    * elm_list_item_append(li, "First", NULL, NULL, NULL, NULL);
    * elm_list_item_append(li, "Second", NULL, NULL, NULL, NULL);
    * elm_list_go(li);
    * evas_object_show(li);
